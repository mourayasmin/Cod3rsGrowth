<mvc:View controllerName="ui5.wwwroot.app.Marca.ListaDeMarcas"
  xmlns="sap.m"
  xmlns:core ="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc">
	<Panel
    headerText="{i18n>helloPanelTitle}"
      class="sapUiResponsiveMargin"
      width="auto">
	</Panel>
	<Table
	id="tabelaListaDeMarcas"
	items="{
        path: 'modelMarcas>/data'}">
		<columns>
			<Column>
				<Text text="{i18n>colunaUmListaDeMarcas}"/>
			</Column>
			<Column>
				<Text text="{i18n>colunaDoisListaDeMarcas}"/>
			</Column>
			<Column>
				<Text text="{i18n>colunaTresListaDeMarcas}"/>
			</Column>
			<Column>
				<Text text="{i18n>colunaQuatroListaDeMarcas}"/>
			</Column>
			<Column>
				<Text text="{i18n>colunaCincoListaDeMarcas}"/>
			</Column>
      </columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{modelMarcas>Email}"/>
					<Text text="{modelMarcas>Nome}"/>
					<Text text="{modelMarcas>CNPJ}"/>
					<Text text="{modelMarcas>Telefone}"/>
					<Text text="{modelMarcas>DataDeCriacao}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</mvc:View>